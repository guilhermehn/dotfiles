{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "space-in-parens": ["error", "never"],
    "arrow-body-style": [2, "as-needed"]
  },
  "env": {
    "es6": true,
    "jquery": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "jquery"],
  "parser": "babel-eslint",
  "root": true
}
