{
  "rules": {
    "indent": ["warn", 2],
    "quotes": ["warn", "single"],
    "linebreak-style": ["warn", "unix"],
    "semi": ["warn", "always"],
    "space-in-parens": ["warn", "never"],
    "array-bracket-spacing": ["warn", "never"],
    "computed-property-spacing": ["warn", "never"],
    "arrow-body-style": ["warn", "as-needed"]
  },
  "env": {
    "jquery": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "jquery"],
  "parser": "babel-eslint",
  "root": true
}
